#!name=策略面板
#!desc=Update-2023.2.20
#!system=ios

[Panel]
# > Surge Pro标题,可显示启动时间,点击刷新为重载配置
#SurgePro_ReloadProfile = script-name=SurgePro_ReloadProfile, update-interval=1

# > 详细的显示你当前的网络信息
NetInfoPanel=title="",content="",style=info,script-name=net-info-panel.js,update-interval=1

# > 流媒体解锁检测
Stream-All-1 = script-name=Stream-All-1,update-interval=3600

# > 机场信息
Airport_Information = script-name=Airport_Information,update-interval=3600

# > 流量统计
TrafficStatistics = script-name=TrafficStatistics, update-interval=1

# > ☁️ WARP Panel
☁️ WARP Panel = title="☁️ 𝙒𝘼𝙍𝙋 𝙄𝙣𝙛𝙤", icon="lock.icloud.fill", script-name=☁️ WARP Panel

# > 网络信息
Network_Information = script-name=Network_Information,update-interval=1

[Script]
# > 附带重载配置,Surge Pro标题,可显示启动时间,点击刷新为重载配置
#SurgePro_ReloadProfile = type=generic, timeout=10, script-path = https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/surgepro_reloadprofile.js, argument=icon=crown.fill&color=#f6c970

# > 详细的显示你当前的网络信息
net-info-panel.js=script-path=https://raw.githubusercontent.com/Rabbit-Spec/Surge/Master/Module/Panel/Network-Info/Moore/Network-Info.js,type=generic,timeout=30

# > 流媒体解锁检测
Stream-All-1 = type=generic,timeout=30,script-path=https://raw.githubusercontent.com/w164955/Config/main/JavaScript/Stream-All-1.js,argument=title=UnlockDetection&icon=display&color=#ff0000

# > 机场信息
Airport_Information = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/mieqq/mieqq/master/sub_info_panel.js,script-update-interval=0,argument=url=[URL encode 后的机场节点链接]&reset_day=到期时间&title=机场名称&icon=externaldrive.fill.badge.icloud&color=#007aff

# > 流量统计 点击以切换网络界面
TrafficStatistics = type=generic,timeout=10,script-path= https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/trafficstatistics.js, argument=icon=bonjour&color=#007aff

# > WARP Panel
☁️ WARP Panel = type=generic, timeout=15, script-path=https://raw.githubusercontent.com/VirgilClyne/Cloudflare/main/js/Cloudflare.1.1.1.1.panel.js

# > 网络信息
Network_Information = type=generic,timeout=60,script-path=https://raw.githubusercontent.com/xream/scripts/main/surge/modules/network-info/index.js,argument=icon=globe.asia.australia.fill&icon-color=#5d84f8
网络变化 = type=event,event-name=network-changed,timeout=30,script-update-interval=3600,debug=false,enable=true,script-path=https://raw.githubusercontent.com/xream/scripts/main/surge/modules/network-info/event.js

[Host]
#详细的显示你当前的网络信息
ip-api.com = 208.95.112.1
