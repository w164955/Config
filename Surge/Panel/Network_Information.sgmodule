#!name=网络信息
#!desc=Update-2023.2.20
#!system=ios

[Panel]
# > 网络信息
Network_Information = script-name=Network_Information,update-interval=1

[Script]
# > 网络信息
Network_Information = type=generic,timeout=60,script-path=https://raw.githubusercontent.com/xream/scripts/main/surge/modules/network-info/index.js,argument=icon=globe.asia.australia.fill&icon-color=#5d84f8
网络变化 = type=event,event-name=network-changed,timeout=30,script-update-interval=3600,debug=false,enable=true,script-path=https://raw.githubusercontent.com/xream/scripts/main/surge/modules/network-info/event.js

[Host]
#详细的显示你当前的网络信息
ip-api.com = 208.95.112.1
